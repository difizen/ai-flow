{
  "name": "@alipay/ai-workflow",
  "version": "0.0.0",
  "description": "A library project powered by bigfish-library",
  "repository": {
    "type": "git",
    "url": "https://code.alipay.com/ai-platform/ai-workflow"
  },
  "author": "xujianfeng.xjf",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "icons",
    "LEGAL.md",
    "assets.json"
  ],
  "scripts": {
    "build": "bigfish-lib build",
    "build:watch": "bigfish-lib build -w",
    "ci": "tnpm run lint && jest --coverage",
    "cov": "jest --coverage",
    "create": "bigfish-lib create",
    "deploy": "bigfish-lib deploy",
    "dev": "bigfish-lib doc dev",
    "doctor": "bigfish-lib doctor",
    "lint": "bigfish-lib lint",
    "prepare": "bigfish-lib setup && husky install",
    "prepublishOnly": "bigfish-lib prepublish",
    "pull": "bigfish-lib pull",
    "start": "tnpm run dev",
    "tailwind": "bigfish g tailwindcss",
    "test": "jest --watch-all"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --cache --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "bigfish-lib lint --fix --eslint-only",
      "prettier --cache --write"
    ],
    "*.{css,less}": [
      "bigfish-lib lint --fix --stylelint-only",
      "prettier --cache --write"
    ],
    "*.{ts,tsx}": [
      "bigfish-lib lint --fix --eslint-only",
      "prettier --cache --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.5.1",
    "@dagrejs/dagre": "^1.1.4",
    "@floating-ui/react": "^0.26.24",
    "@headlessui/react": "^2.1.9",
    "@lexical/code": "^0.17.1",
    "@lexical/react": "^0.17.1",
    "@lexical/text": "^0.17.1",
    "@lexical/utils": "^0.17.1",
    "@remixicon/react": "^4.2.0",
    "@types/json-schema": "^7.0.15",
    "@xyflow/react": "^12.3.1",
    "ahooks": "^3.8.1",
    "antd": "^5.21.3",
    "classnames": "^2.5.1",
    "immer": "^10.1.1",
    "js-yaml": "^4.1.0",
    "lexical": "^0.17.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hotkeys-hook": "^4.5.1",
    "short-unique-id": "^5.2.0",
    "styled-components": "^6.1.13",
    "tailwind-merge": "^2.5.3",
    "uuid": "^10.0.0",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.5.13",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "tailwindcss": "^3.4.13",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "antd": "^5.0.0",
    "react": ">=16.9.0"
  },
  "optionalDependencies": {
    "node-bin-darwin-arm64": "16",
    "node-darwin-x64": "16",
    "node-linux-arm64": "16",
    "node-linux-x64": "16",
    "node-win-x64": "16",
    "node-win-x86": "16"
  },
  "engines": {
    "install-node": "16"
  },
  "publishConfig": {
    "registry": "https://registry.antgroup-inc.cn"
  },
  "ci": {
    "type": "aci"
  },
  "dumiAssets": "assets.json",
  "tnpm": {
    "mode": "npm"
  },
  "yuyanId": "180020010101322476"
