{
  "name": "@alipay/ai-workflow",
  "version": "0.0.0",
  "description": "A library project powered by bigfish-library",
  "repository": {
    "type": "git",
    "url": "https://code.alipay.com/ai-platform/ai-workflow"
  },
  "author": "xujianfeng.xjf",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "icons",
    "LEGAL.md",
    "assets.json"
  ],
  "scripts": {
    "build": "bigfish-lib build",
    "build:watch": "bigfish-lib build -w",
    "ci": "tnpm run lint && jest --coverage",
    "cov": "jest --coverage",
    "create": "bigfish-lib create",
    "deploy": "bigfish-lib deploy",
    "dev": "bigfish-lib doc dev",
    "doctor": "bigfish-lib doctor",
    "lint": "bigfish-lib lint",
    "prepare": "bigfish-lib setup && husky install",
    "prepublishOnly": "bigfish-lib prepublish",
    "pull": "bigfish-lib pull",
    "start": "tnpm run dev",
    "tailwind": "bigfish g tailwindcss",
    "test": "jest --watch-all"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --cache --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "bigfish-lib lint --fix --eslint-only",
      "prettier --cache --write"
    ],
    "*.{css,less}": [
      "bigfish-lib lint --fix --stylelint-only",
      "prettier --cache --write"
    ],
    "*.{ts,tsx}": [
      "bigfish-lib lint --fix --eslint-only",
      "prettier --cache --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.0",
    "@babel/runtime": "^7.18.0",
    "@types/json-schema": "^7.0.15",
    "@xyflow/react": "^12.0.2",
    "ajv": "^8.17.1",
    "lodash": "^4.17.21",
    "react-hotkeys-hook": "^4.5.0",
    "short-unique-id": "^5.2.0",
    "styled-components": "^6.0.7",
    "uuid": "^10.0.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@ali/ci": "^4.26.0",
    "@alipay/bigfish-library": "^4.0.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "antd": "^5.19.2",
    "husky": "^8.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.0.0",
    "prettier": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^3"
  },
  "peerDependencies": {
    "antd": "^5.0.0",
    "react": ">=16.9.0"
  },
  "engines": {
    "install-node": "16"
  },
  "publishConfig": {
    "registry": "https://registry.antgroup-inc.cn"
  },
  "ci": {
    "type": "aci"
  },
  "dumiAssets": "assets.json",
  "tnpm": {
    "mode": "npm"
  },
  "yuyanId": "180020010101322476"
}
